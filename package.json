{
  "name": "angularjs-testbed",
  "version": "0.1.3",
  "license": "MIT",
  "author": {
    "name": "Vlad Sternbach",
    "email": "vlad.sternbach@gmail.com",
    "url": "https://github.com/vsternbach"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsternbach/angularjs-testbed.git"
  },
  "homepage": "https://github.com/vsternbach/angularjs-testbed",
  "keywords": [
    "angular testbed",
    "angular-testbed",
    "jasmine",
    "karma",
    "testing",
    "testbed",
    "angularjs-testbed",
    "component testing",
    "anguarjs testing"
  ],
  "main": "angularjs-testbed.umd.js",
  "module": "angularjs-testbed.js",
  "jsnext:main": "angularjs-testbed.js",
  "types": "types/index.d.ts",
  "dependencies": {
    "angular-ts-decorators": "3.0.5",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/angular": "^1.6.24",
    "@types/angular-mocks": "^1.5.11",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^8.5.2",
    "angular": "^1.6.0",
    "angular-mocks": "^1.6.3",
    "copyfiles": "^1.2.0",
    "coveralls": "^2.13.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-typescript": "^3.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-typescript2": "^0.10.0",
    "rollup-plugin-uglify": "^3.0.0",
    "standard-version": "^4.0.0",
    "tslint": "^5.1.0",
    "typescript": "^2.4.1",
    "uglify-es": "^3.0.4"
  },
  "peerDependencies": {
    "angular": ">=1.5.0",
    "angular-mocks": ">=1.5.0"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "test": "karma start --single-run",
    "test:dev": "karma start",
    "posttest": "cat ./coverage/lcov.info | coveralls",
    "build": "npm run clean && tsc && rollup -c && npm run uglify && copyfiles package.json README.md CHANGELOG.md LICENSE dist/ && rimraf dist/temp",
    "newbuild": "npm run clean && rollup -c && copyfiles package.json README.md CHANGELOG.md LICENSE dist/",
    "uglify": "for f in dist/*.js; do ./node_modules/uglify-es/bin/uglifyjs $f --compress drop_console --mangle --output ${f%.js}.min.js; done",
    "release": "standard-version",
    "postrelease": "git push origin master --follow-tags"
  }
}
